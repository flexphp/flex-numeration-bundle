{% for _provider in providers %}
    <tr>
        <td>{{ _provider.id }}</td>
        <td>{{ _provider.type }}</td>
        <td>{{ _provider.name }}</td>
        <td>{{ _provider.username }}</td>
        <!-- <td>{{ _provider.password }}</td> -->
        <td>{{ _provider.url }}</td>
        <td>{% if _provider.isActive %}{% trans from 'messages' %}label.yes{% endtrans %}{% else %}{% trans from 'messages' %}label.no{% endtrans %}{% endif %}</td>
        <td class="text-center">
            <div class="btn-group">
                <a href="{{ path('flexphp.numeration.providers.read', {id: _provider.id}) }}" class="btn btn-sm btn-outline-light" title="{% trans from 'messages' %}action.read{% endtrans %}">
                    <i class="fa fa-eye text-dark" aria-hidden="true"></i>
                </a>

                <a href="{{ path('flexphp.numeration.providers.edit', {id: _provider.id}) }}" class="btn btn-sm btn-outline-light" title="{% trans from 'messages' %}action.edit{% endtrans %}">
                    <i class="fa fa-edit text-info" aria-hidden="true"></i>
                </a>
            </div>
        </td>
    </tr>
{% endfor %}
